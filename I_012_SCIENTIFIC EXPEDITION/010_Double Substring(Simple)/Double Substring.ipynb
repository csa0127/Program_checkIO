   "source": [
    "find a length the longest substring that repeats more than once. "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 47,
   "metadata": {},
   "outputs": [],
   "source": [
    "import re\n",
    "def double_substring(line):\n",
    "    matches = re.findall(r'(.+)(?=.*\\1)',line)\n",
    "    return max([len(i) for i in matches]) if matches else 0"
   ]
  },

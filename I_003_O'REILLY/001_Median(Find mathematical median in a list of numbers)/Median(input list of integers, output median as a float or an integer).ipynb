    "def checkio(data):\n",
    "    d = sorted(data)\n",
    "    le = len(data)\n",
    "    return (d[int((le-1)/2)]) if le%2==1 else ((d[int(le/2)]+d[int(le/2 - 1)]))/2"
   ]
